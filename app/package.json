    "build": "cross-env REACT_APP_ENDPOINT=https://mo-hae.site/ node scripts/build.js",
    "test": "node scripts/test.js",
    "serve": "cross-env REACT_APP_ENDPOINT=https://mo-hae.site/ npx serve -l 8080 -s build -n"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.5",
    "envify": "^4.1.0",
    "json-server": "^0.17.0",
